#include <stdio.h>

int main() {
    int tabuleiro[10][10];
    int i, j;

    for (i = 0; i < 10; i++) {
        for (j = 0; j < 10; j++) {
            tabuleiro[i][j] = 0;
        }
    }

   
    int linhaH = 2, colunaH = 3;
    for (j = 0; j < 3; j++) {
        tabuleiro[linhaH][colunaH + j] = 3;
    }

  
    int linhaV = 5, colunaV = 5;
    for (i = 0; i < 3; i++) {
        tabuleiro[linhaV + i][colunaV] = 3;
    }

    for (i = 0; i < 10; i++) {
        for (j = 0; j < 10; j++) {
            printf("%d ", tabuleiro[i][j]);
        }
        printf("\n");
    }

    return 0;
}
